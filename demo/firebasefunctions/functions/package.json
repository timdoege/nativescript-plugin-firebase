    "firebase-functions": "~3.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "firebase-functions-test": "^0.1.7",
    "mocha": "^6.2.2",
    "tslint": "~5.20.1",
    "typescript": "3.5.3"
  },
  "private": true
